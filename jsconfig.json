{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@context/*": [
                "src/context/*"
            ],
            "@loaders/*": [
                "src/loaders/*"
            ],
            "@fetch/*": [
                "src/fetch/*"
            ]
        }
    }
}