{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@inlineAlerts/*": [
                "src/components/inlineAlerts/*"
            ],
            "@context/*": [
                "src/context/*"
            ],
            "@email-verification/*": [
                "src/pages/email-verification/*"
            ],
            "@settings/*": [
                "src/pages/settings/*"
            ],
            "@changelog/*": [
                "src/pages/changelog/*"
            ],
            "@lists/*": [
                "src/pages/lists/*"
            ]
        }
    }
}